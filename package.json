{
  "name": "ssr-react",
  "version": "1.0.0",
  "description": "Webpack gym ",
  "main": "index.js",
  "scripts": {
    "build:externals": "webpack --config ./externals/webpack.config.js",
    "build:multi": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:url-loader": "webpack-dev-server --config ./url-loader/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "MisaelCalvillo",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.6.0",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/plugin-transform-runtime": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "2.1.1",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "style-loader": "1.0.0",
    "url-loader": "2.1.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.8.0"
  }
}
